        "ConnectionStrings": [
          "127.0.0.1:6379,defaultDatabase=13,poolsize=10"
        ],
        "Sentinels": [
          "192.169.1.10:26379",
          "192.169.1.11:26379",
          "192.169.1.12:26379"
        ],
        "ReadOnly": false
      }
    }
  }
}
